<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beautyshopapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BeautyShopApp">
        <activity
            android:name=".ui.home.QuotesAppointment"
            android:exported="false"
            android:label="Citas pendientes"/>
        <activity
            android:name=".ui.fab.quoteActivity"
            android:exported="false"
            android:label="Nueva cita"/>
        <activity
            android:name=".ui.fab.spentActivity"
            android:exported="false"
            android:label="Nuevo gasto"/>
        <activity
            android:name=".ui.fab.saleActivity"
            android:exported="false"
            android:label="Nueva venta" />
        <activity
            android:name=".ui.home.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".SeccionCuenta"
            android:exported="false" />
        <activity
            android:name=".ui.gallery.EntryProductActivity"
            android:exported="false" />
        <activity
            android:name=".ui.gallery.DetailsActivity"
            android:exported="false"
            android:label="Resumen producto"/>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.BeautyShopApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>